<% if key = params[:key] %>

  <h1>Key "<%= key %>" is a <%= resque.redis.type key %></h1>
  <h2>size: <%= redis_get_size(key) %></h2>
  <table>
    <% for row in redis_get_value_as_array(key) %>
    <tr>
      <td>
        <%= row %>
      </td>
    </tr>
    <% end %>
  </table>

<% else %>

<% end %>